<%= paginate @users %>

<table>
  <tr>
    <th>ID</th> 
    <th>昵称</th<>
    <th>邮箱</th> 
    <th>注册时间</th> 
    <th>是否激活</th> 
    <th col='3'>操作</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.nick_name %></td>
    <td><%= link_to "#{user.email}", loyal_passport_app.admin_user_url(:id => user.id) %></td>
    <td><%= l user.created_at %></td>
    <td><%= user.confirmed? %></td>
    <td><%= link_to "编辑", loyal_passport_app.edit_admin_user_url(:id => user.id) %></td>
  </tr>
<% end %>

</table>

<%= paginate @users %>
